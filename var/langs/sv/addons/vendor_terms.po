msgctxt "Addons::name::vendor_terms"
msgid "Vendor's terms and conditions"
msgstr "Leverantörens regler och villkor"

msgctxt "Addons::description::vendor_terms"
msgid "Allows vendors to specify terms and conditions that the customers must agree with when buying from those vendors"
msgstr ""

msgctxt "Languages::vendor_terms.terms_and_conditions"
msgid "Terms & conditions"
msgstr "Köpvillkor"

msgctxt "Languages::vendor_terms.terms_and_conditions_tooltip"
msgid "If you want a customer to agree to your terms and conditions at checkout, fill in Terms & Conditions. If you leave Terms & Conditions empty, a customer proceeds without agreement."
msgstr ""

msgctxt "Languages::vendor_terms.checkout_terms_and_conditions"
msgid "I accept the [terms_href] of [vendor]"
msgstr "Jag accepterar [terms_href] av [vendor]"

msgctxt "Languages::vendor_terms.checkout_terms_and_conditions_name"
msgid "Terms & conditions"
msgstr "Köpvillkor"

